<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dai.categoryexample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.DataBindingActivity" />
        <activity android:name=".activity.StorageActivity" />
        <activity android:name=".activity.DialogActivity" />
        <activity android:name=".activity.ConstraintActivity" />
        <activity android:name=".activity.PermissionActivity" />


        <service
            android:name=".service.ForeService"/>
    </application>

    <uses-permission android:name="android.permission.READ_CALENDAR"/>

</manifest>