<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dai.categoryexample">

    <uses-permission android:name="android.permission.READ_CALENDAR" />

    <!-- To access Google+ APIs: -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/os_apple"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.DataBindingActivity" />
        <activity android:name=".activity.StorageActivity" />
        <activity android:name=".activity.DialogActivity" />
        <activity android:name=".activity.ConstraintActivity" />
        <activity android:name=".activity.PermissionActivity" />
        <activity android:name=".activity.ParamsActivity" />
        <activity android:name=".activity.FragmentManagerActivity" />

        <service android:name=".service.ForeService" />

        <activity
            android:name=".activity.RecyclerActivity"
            android:label="@string/title_activity_recycler" />

        <receiver android:name=".receiver.OreoReceiver">
            <intent-filter>
                <action android:name="android.intent.action.test.oreo" />
            </intent-filter>
        </receiver>

        <activity android:name=".activity.EmptyActivity"></activity>
    </application>

</manifest>